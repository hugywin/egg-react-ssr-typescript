{
    "name": "egg-react-ssr-typescript",
    "version": "1.0.0",
    "description": "SSR scaffolding using typescript in egg server environment",
    "private": true,
    "egg": {
        "typescript": true,
        "declarations": true
    },
    "scripts": {
        "start": "egg-scripts start --daemon --title=egg-server-egg-react-ssr-typescript",
        "stop": "egg-scripts stop --title=egg-server-egg-react-ssr-typescript",
        "dev": "egg-bin dev",
        "debug": "egg-bin debug",
        "test-local": "egg-bin test",
        "test": "npm run lint -- --fix && npm run test-local",
        "cov": "egg-bin cov",
        "tsc": "ets && tsc -p tsconfig.json",
        "ci": "npm run lint && npm run cov && npm run tsc",
        "autod": "autod",
        "lint": "tslint --project . -c tslint.json",
        "clean": "ets clean"
    },
    "dependencies": {
        "egg": "^2.6.1",
        "egg-scripts": "^2.6.0",
        "react": "^16.8.3",
        "react-dev-utils": "^8.0.0",
        "react-dom": "^16.8.3",
        "react-router-dom": "^5.0.0",
        "serialize-javascript": "^1.6.1"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.40",
        "@types/node": "^7.0.12",
        "@types/supertest": "^2.0.0",
        "autod": "^3.0.1",
        "autod-egg": "^1.1.0",
        "egg-bin": "^4.13.1",
        "egg-ci": "^1.8.0",
        "egg-mock": "^3.16.0",
        "tslib": "^1.9.0",
        "tslint": "^5.0.0",
        "tslint-config-egg": "^1.0.0",
        "typescript": "^3.0.0"
    },
    "engines": {
        "node": ">=8.9.0"
    },
    "ci": {
        "version": "8"
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "eslintIgnore": [
        "coverage"
    ],
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "author": "zhusjfaker",
    "license": "MIT"
}